{
	"model": {
		"load_path": "best_supervised",
		"batchsz": 1000,
		"seed": 0,
		"epoch": 200,
		"eval_frequency": 5,
		"process_num": 8,
		"use_masking": true,
		"shrink": false,
		"use_state_entropy": false,
		"use_state_mutual_info": false,
		"use_confidence_scores": false,
		"sys_semantic_to_usr": false,
		"num_eval_dialogues": 500
	},
	"nlu_sys": {},
	"dst_sys": {
		"setsumbt-mul": {
			"class_path": "convlab2.dst.setsumbt.multiwoz.Tracker.SetSUMBTTracker",
			"ini_params": {
				"model_path": "convlab_setsumbt_acts_distilled",
				"get_confidence_scores": false,
				"return_mutual_info": false,
				"return_entropy": false
			}
		}
	},
	"sys_nlg": {
		"TemplateNLG": {
			"class_path": "convlab2.nlg.template.multiwoz.TemplateNLG",
			"ini_params": {
				"is_user": false
			}
		}
	},
	"nlu_usr": {
		"BERTNLU": {
			"class_path": "convlab2.nlu.jointBERT.multiwoz.BERTNLU",
			"ini_params": {
				"mode": "sys",
				"config_file": "multiwoz_sys_context.json",
				"model_file": "https://convlab.blob.core.windows.net/convlab-2/bert_multiwoz_sys_context.zip"
			}
		}
	},
	"dst_usr": {},
	"policy_usr": {
		"RulePolicy": {
			"class_path": "convlab2.policy.rule.multiwoz.RulePolicy",
			"ini_params": {
				"character": "usr"
			}
		}
	},
	"usr_nlg": {
		"TemplateNLG": {
			"class_path": "convlab2.nlg.template.multiwoz.TemplateNLG",
			"ini_params": {
				"is_user": true,
				"label_noise": 0.0,
				"text_noise": 0.0,
				"seed": 0
			}
		}
	}
}